{
	"schemaVersion": 1,
	"id": "wanteditems",
	"version": "${version}",
	"name": "Wanted Items",
	"description": "Cobblemon side mod adding special items for server admins",
	"authors": [
		"KiwiFlavoredApollo"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "GPL 3.0",
	"icon": "assets/wanteditems/icon.png",
	"environment": "*",
	"entrypoints": {
		"fabric-datagen": [
			"kiwiapollo.wanteditems.datagen.DataGenerator"
		],
		"main": [
			"kiwiapollo.wanteditems.WantedItems"
		],
		"client": [
			"kiwiapollo.wanteditems.WantedItemsClient"
		]
	},
	"mixins": [
		"wanteditems.mixins.json",
		{
			"config": "wanteditems.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"cobblemon": "1.5.2"
	},
	"suggests": {
		"mythsandlegends": "*"
	}
}